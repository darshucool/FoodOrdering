        function getGuestCount() {
            var count = $("#memcount").val();
            var rank = $("#RankVal").val();
            rank = parseInt(rank);
            count = parseInt(count);

            if (rank == 27)
            {

                if (count > 4) {
                    alert("you are not allowed to enter more than 4 guests");
                    $("#memcount").empty();
                    $("#memcount").val(4);
                }
                else if (count <= 4)  {
                    $("#memcount").val(count);
                }
            }
            else if(rank == 10)
            {
                if (count > 4) {
                    alert("you are not allowed to enter more than 4 guests");
                    $("#memcount").empty();
                    $("#memcount").val(4);
                }
                else if (count <= 4)  {
                    $("#memcount").val(count);
                }
            }
            else if(rank == 147)
            {
                if (count > 4) {
                    alert("you are not allowed to enter more than 4 guests");
                    $("#memcount").empty();
                    $("#memcount").val(4);
                }
                else if (count <= 4)  {
                    $("#memcount").val(count);
                }
            }
            else if (rank >= 28 && rank <= 44) {

                if (count > 2) {
                    alert("you are not allowed to enter more than 2 guests");
                    $("#memcount").empty();
                    $("#memcount").val(2);
                }
                else if (count <= 2) {
                    $("#memcount").val(count);
                }
            }

        }
